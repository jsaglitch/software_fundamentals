<?xml version="1.0"?>
<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="Reto_condicional_4"/>
        <attribute name="authors" value="Jaider"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-11-15 04:07:10 p.&#160;m."/>
